{
	"name": "vscode-moqui-support",
	"displayName": "vscode moqui support",
	"description": "Support moqui xml files",
	"author": {
		"name": "Yao Chunlin",
		"url": "https://github.com/chunlinyao/vscode-moqui-support.git",
		"email": "chunlinyao@gmail.com"
	},
	"license": "MulanPSL-2.0",
	"publisher": "chunlinyao",
	"version": "0.0.1",
	"repository": {
		"type": "git",
		"url": "https://github.com/chunlinyao/vscode-moqui-support.git"
	},
	"engines": {
		"vscode": "^1.65.0"
	},
	"categories": [
		"Programming Languages"
	],
	"keywords": [
		"moqui",
		"xml"
	],
	"activationEvents": [
		"onCommand:vscode-moqui-support.helloWorld"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "vscode-moqui-support.helloWorld",
				"title": "Hello World"
			}
		],
		"grammars": [
			{
				"injectTo": [
					"text.xml"
				],
				"scopeName": "embedded.moqui-groovy",
				"path": "./syntaxes/moqui-groovy.tmLanguage.json",
				"embeddedLanguages": {
					"meta.embedded.moqui.groovy": "groovy",
					"source.groovy.embedde.source": "groovy"
				}
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run esbuild-base -- --minify",
		"esbuild-base": "esbuild ./src/extension.ts --bundle --outfile=./out/extension.js --external:vscode --format=cjs --platform=node",
		"esbuild": "npm run esbuild-base -- --sourcemap",
		"esbuild-watch": "npm run esbuild-base -- --sourcemap --watch",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"lint": "eslint src --ext ts",
		"test": "node ./out/test/runTest.js",
		"package": "vsce package"
	},
	"devDependencies": {
		"@types/vscode": "^1.65.0",
		"@types/glob": "^7.2.0",
		"@types/mocha": "^9.1.0",
		"@types/node": "14.x",
		"@typescript-eslint/eslint-plugin": "^5.12.1",
		"@typescript-eslint/parser": "^5.12.1",
		"eslint": "^8.9.0",
		"glob": "^7.2.0",
		"mocha": "^9.2.1",
		"typescript": "^4.5.5",
		"@vscode/test-electron": "^2.1.2"
	}
}
